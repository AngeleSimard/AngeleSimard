import pandas as pd

data = {
    'Software/Subject': [
        'IBM Cognos Analytics', 'IBM Watson Studio', 'Jupyter Notebooks', 
        'Python', 'Excel', 'Powerpoint', 'Akkio', 'ChartPixel', 
        'Accounting', 'Economics', 'Budgeting', 'Financial Planning'
    ],
    'Rating': [5, 4, 2, 2, 5, 5, 3, 3, 4, 4, 5, 5]
}

df = pd.DataFrame(data)
print(df)
from wordcloud import WordCloud
import matplotlib.pyplot as plt

# Data
data = {
    'IBM Cognos Analytics': 5, 'IBM Watson Studio': 4, 'Jupyter Notebooks': 2, 
    'Python': 2, 'Excel': 5, 'Powerpoint': 5, 'Akkio': 3, 'ChartPixel': 3, 
    'Accounting': 4, 'Economics': 4, 'Budgeting': 5, 'Financial Planning': 5
}

# Create word cloud
wordcloud = WordCloud(width=800, height=400, background_color='white', colormap='Blues').generate_from_frequencies(data)

# Display the word cloud
plt.figure(figsize=(10, 8))
plt.imshow(wordcloud, interpolation='bilinear')
plt.axis('off')
plt.title('Word Cloud of Software/Subjects Ratings')
plt.show()
